{
  "manifest_version": 3,
  "name": "GitHub Shortcut Helper",
  "version": "1.0",
  "description": "Shows keyboard shortcuts for GitHub when clicked",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["dist/content.js"],
      "css": ["src/styles.css"]
    }
  ]
}